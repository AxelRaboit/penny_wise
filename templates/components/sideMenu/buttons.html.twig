{% for button in buttons %}
    <twig:SideMenuCta
        id="{{ button.id }}"
        color="quaternary"
        size="md"
        fullWidth="false"
        extraClasses="mb-2"
        icon="{{ button.icon }}"
        label="{{ button.text is defined ? button.text : '' }}"
        title="{{ button.title }}"
        dataActionUrl="true"
    />
{% endfor %}
