{% set categories = ['incomes', 'bills', 'expenses', 'debts'] %}

<div class="grid grid-cols-1 md:grid-cols-1 lg:grid-cols-1 xl:grid-cols-2 xxl:grid-cols-4 gap-1">
    {% for category in categories %}
        <div class="flex flex-col">
            {{ include('wallet/partial/transaction/_transaction.html.twig', {transactionCategory: attribute(transactionCategories, category)}) }}
        </div>
    {% endfor %}
</div>
